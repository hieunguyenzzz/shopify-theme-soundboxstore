<style>
   .header-desktop-nav-moreinfo  {
     white-space: normal;
     opacity:1;font-sizr:14px;
  }
    .header-desktop-nav-moreinfo a  {
     padding:0;
   margin :0
  }
    .header-desktop-nav-moreinfo a:hover{
        text-decoration: none;
    }
   .header-desktop-nav-moreinfo ul  {
     padding:0;
   margin :0
  }
  .header-desktop-nav-moreinfo-col1{
    padding: 8px 40px 40px 0;border-right:2px solid;
  }
  .header-desktop-nav-moreinfo-col2{
    padding: 8px 40px 40px 40px;border-right:2px solid;
  }
   .header-desktop-nav-moreinfo-col3{
    padding: 8px 0 40px 40px;
  }
        .header-desktop-nav-moreinfo-menu a,.header-desktop-nav-moreinf-title{
     line-height:1.3;font-weight:600;text-transform:uppercase;font-size: 18px;
    text-transform: uppercase;
    font-weight: 500;
          padding-bottom:0.5em;font-family: "Banana Grotesk", var(--typeBasePrimary), var(--typeBaseFallback), system-ui;
  }
  .header-desktop-nav-moreinfo-bottom{
    border-top: 2px solid;
    padding-top:1em
  }
     .col_1 {
        position: relative;background:white;
      text-align:left
    }

    .col_1-label {
    position: absolute;
    right: -0.5em;
    top: 0;
    font-size: 0.8em;
    color: white;
    padding: 0.2em 1em;
    border-radius: 99em;
    line-height: 1.5em;
    background: #fb4646;
    margin-top: -1em;
    }

    .col_1-content-inner {
        display: flex;
        gap: 1.4em;
    }

    .col_1-subtitle {
        font-size: 0.9em;
      opacity:0.4;
        margin-block-start: 0.5em;
    }

    .col_1-title {
      font-size: 1.1em; white-space:nowrap;font-weight:500
    }
    .col_1-content-icon {
      width: 1.5em;
      height: 1.5em;
      display: flex;
      justify-content: center;
      align-items: center;
  }
  .col_1-content-icon svg {
     font-size:1.4em
  }
</style>

{%- unless title -%}
  {%- assign title = settings.header_moreinfo -%}
{%- endunless -%}
{% capture col_1 %}
   <ul class=" header-desktop-nav-moreinfo-menu">
            {%- for menu in sub_menu.links -%}
              <li
                class=""
                aria-haspopup="true"
              >
                <a
                  href="{{ menu.url }}"
                  class=""
                >
                  {{ menu.title }}
                </a>
                {% if menu.links.size > 1 %}
                  <div class="" style="min-height: unset;">
                    <div
                      class=""
                    >
                      <ul style="margin:0">
                        {%- for menu in menu.links -%}
                          <li
                            class=""
                            aria-haspopup="true"
                          >
                            <a
                              href="{{ menu.url }}"
                            >
                              {{ menu.title }}
                            </a>
                          </li>
                        {% endfor %}
                      </ul>
                    </div>
                  </div>
                {% endif %}
              </li>
            {% endfor %}
          </ul>
  {% endcapture %}
{% capture col_2 %}
  <div style="display:flex; flex-direction:column;align-items:center;text-align:center">
      <a class="specBtn" style="display:flex; justify-content:center;margin-bottom:0.5em">
        {% if settings.header_guide_image %}
          {{ settings.header_guide_image |  image_url: width: 500 |  image_tag : style:"background: var(--image-bg);border-radius: 1em;    height:250px;" }}
          {% else %}
            <img
          style="
              height:250px;
            background: var(--image-bg);
            border-radius: 1em;
          "
          src="https://cdn.shopify.com/s/files/1/0631/9892/0929/files/folio-brochure-graph.png?v=1700815295"
        >
        {% endif %}
      </a>
      <div
        style="flex:1;display: flex;flex-direction: column;justify-content: center;"
      >
        <div style="font-size: 1.4em;line-height: 1.3;font-weight: 500;margin-block: 0.5em;">
          {{settings.header_guide_title}}
        </div>
        <div>
          {{settings.header_guide_description}}
        </div>
      </div>

      <button
        class="btn specBtn"
        style="
          margin-block: 1.4em;
          display: flex;
          align-items: center;
        "
      >
        <div
          class=""
          style="
            display: inline-flex;
            font-size: 1.4em;
            height: 0;
            align-items: center;
            margin-right: 0.6em;
          "
        >
          <svg
            stroke="currentColor"
            fill="currentColor"
            stroke-width="0"
            viewBox="0 0 20 20"
            aria-hidden="true"
            height="1em"
            width="1em"
            xmlns="http://www.w3.org/2000/svg"
            style="display: inline;line-height: 0;display: block;"
          >
            <path fill-rule="evenodd" d="M3 17a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zm3.293-7.707a1 1 0 011.414 0L9 10.586V3a1 1 0 112 0v7.586l1.293-1.293a1 1 0 111.414 1.414l-3 3a1 1 0 01-1.414 0l-3-3a1 1 0 010-1.414z" clip-rule="evenodd"></path>
          </svg>
        </div>
        {{  settings.header_guide_button}}
      </button>
      </div>
    {% endcapture %}
{% capture col_3 %}
  <div class="quote-form-modal-content-right-inner">
        
          <button class="col_1">
            <div class="col_1-content">
              <div class="col_1-content-inner">
                <div class="">
                  <span class="col_1-content-icon">
                    <svg xmlns="http://www.w3.org/2000/svg" width="1em" height="1em" viewBox="0 0 576 512">
                      <path fill="currentColor" d="M416 192c0-88.4-93.1-160-208-160S0 103.6 0 192c0 34.3 14.1 65.9 38 92c-13.4 30.2-35.5 54.2-35.8 54.5c-2.2 2.3-2.8 5.7-1.5 8.7S4.8 352 8 352c36.6 0 66.9-12.3 88.7-25c32.2 15.7 70.3 25 111.3 25c114.9 0 208-71.6 208-160m122 220c23.9-26 38-57.7 38-92c0-66.9-53.5-124.2-129.3-148.1c.9 6.6 1.3 13.3 1.3 20.1c0 105.9-107.7 192-240 192c-10.8 0-21.3-.8-31.7-1.9C207.8 439.6 281.8 480 368 480c41 0 79.1-9.2 111.3-25c21.8 12.7 52.1 25 88.7 25c3.2 0 6.1-1.9 7.3-4.8c1.3-2.9.7-6.3-1.5-8.7c-.3-.3-22.4-24.2-35.8-54.5"></path>
                    </svg>
                  </span>
                </div>
                <div class="">
                  <div class="col_1-title">Live chat</div>
                  <div class="col_1-subtitle">M–F, 9am–5pm PT</div>
                </div>
              </div>
            </div>
          </button>
          <button class="col_1">
            <div class="col_1-content">
              <div class="col_1-content-inner">
                <div class="col_1-content-icon">
                  <span
                    class=""
                    style="
                      width: 1.5em;
                      height: 1.5em;
                      display: flex;
                      justify-content: center;
                      align-items: center;
                    "
                  >
                    <svg xmlns="http://www.w3.org/2000/svg" width="1em" height="1em" viewBox="0 0 512 512">
                      <path fill="currentColor" d="m497.39 361.8l-112-48a24 24 0 0 0-28 6.9l-49.6 60.6A370.66 370.66 0 0 1 130.6 204.11l60.6-49.6a23.94 23.94 0 0 0 6.9-28l-48-112A24.16 24.16 0 0 0 122.6.61l-104 24A24 24 0 0 0 0 48c0 256.5 207.9 464 464 464a24 24 0 0 0 23.4-18.6l24-104a24.29 24.29 0 0 0-14.01-27.6"></path>
                    </svg>
                  </span>
                </div>
                <div class="">
                  <a class="col_1-title" href="tel: +44 (0) 20 4586 3800">+44 (0) 20 4586 3800</a>
                  <div class="col_1-subtitle">M–F, 9am–5pm PT</div>
                </div>
              </div>
            </div>
          </button>
    <button class="col_1">
            <div class="col_1-content">
              <div class="col_1-content-inner">
                <div class="col_1-content-icon">
                  <span
                    class=""
                    style="
                      width: 1.5em;
                      height: 1.5em;
                      display: flex;
                      justify-content: center;
                      align-items: center;
                    "
                  >
                    <svg xmlns="http://www.w3.org/2000/svg" width="1em" height="1em" viewBox="0 0 512 512">
                      <path fill="currentColor" d="m497.39 361.8l-112-48a24 24 0 0 0-28 6.9l-49.6 60.6A370.66 370.66 0 0 1 130.6 204.11l60.6-49.6a23.94 23.94 0 0 0 6.9-28l-48-112A24.16 24.16 0 0 0 122.6.61l-104 24A24 24 0 0 0 0 48c0 256.5 207.9 464 464 464a24 24 0 0 0 23.4-18.6l24-104a24.29 24.29 0 0 0-14.01-27.6"></path>
                    </svg>
                  </span>
                </div>
                <div class="">
                  <a class="col_1-title" href="mailto:hush@soundboxstore.com">hush@soundboxstore.com</a>
                  <div class="col_1-subtitle">Email between 9am-5pm PT, and we'll reply the same day.</div>
                </div>
              </div>
            </div>
          </button>
        </div>
    {% endcapture %}
<li
  class="site-nav__item site-nav__item_open site-nav__item-verticle site-nav__expanded-item site-nav--has-dropdown  site-nav--is-megamenu"
  aria-haspopup="true"
>
  <a
    href="/pages/about-us"
    style="text-transform: uppercase;"
    class="site-nav__link site-nav__link--underline site-nav__link--has-dropdown"
  >
    {{ title }}
  </a>
  <div
    class="site-nav__dropdown site-nav__dropdown-animate  header-desktop-nav-moreinfo"
    style="
      padding-bottom: 64px;
      padding-top:56px;
      font-size: 14px;width:100%;
    "
  >
    <div class="page-width site-nav__dropdown-menu">
      <div style="display:flex; flex-direction:column">
        <div style="display:flex;width:100%">
          <div class="header-desktop-nav-moreinfo-col1" style="flex:1">{{ col_1 }}</div>
          <div class="header-desktop-nav-moreinfo-col2" style="flex:1">{{ col_2 }}</div>
          <div class="header-desktop-nav-moreinfo-col3" style="flex:1">{{ col_3 }}</div>
        </div>
        <div class="header-desktop-nav-moreinfo-bottom">
          <div>{{  settings.header_delivery_message}}</div>
          <div
            class="site-nav__dropdown-menu_left-icons"
            style="display: flex;margin-top: 1em;gap: 1em;flex-wrap: wrap;"
          >
            {% render 'iso-images' %}
          </div>
        </div>
      </div>
    </div>
  </div>
</li>
