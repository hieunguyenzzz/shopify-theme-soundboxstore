<script>
 setInterval(function(){
   if(document.querySelector('input#Country-1668755629810') !== null){
     let locale = Shopify.locale;
     let translations = GRFQConfigs.translationsFormbuilder.filter((item => item.element_name == "Country"));
     if(translations.length){
       let trans = JSON.parse(translations[0].translations);
       let listLang = trans.values;
       listLang.forEach(function(item){
         document.querySelector(`select#Country option[value="${item.label}"]`).textContent = item.value;
       })
     }
   }
 },500)
</script>
