{% comment %}
  {% if template contains 'index' %}
    {% if settings.Desk_img != blank %}
      <link rel="preload" as="image" href="{{ settings.Desk_img }}">
    {% endif %}
    {% if settings.mob_img != blank %}
      <link rel="preload" as="image" href="{{ settings.mob_img }}">
    {% endif %}
  {% endif %}
{% endcomment %}

{% if template contains 'product' %}
  <link rel="preload" as="image" href="{{ product.featured_image | img_url: '800x' }}">
{% endif %}

{% if template contains 'collection' %}
  <link rel="preload" as="image" href="{{ collection.products.first.featured_image | img_url: '800x' }}">
{% endif %}

<link rel="preload" href="{{ 'HelveticaNeue.woff2' | file_url }}" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{{ 'BananaGrotesk-Regular.woff2' | file_url }}" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{{ 'BananaGrotesk-Semibold.woff2' | file_url }}" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{{ 'BananaGrotesk-Thin.woff2' | file_url }}" as="font" type="font/woff2" crossorigin>

<link rel="preconnect" href="//fonts.shopifycdn.com/" crossorigin>
