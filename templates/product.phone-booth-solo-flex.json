    "meeting-rooms-features": {
      "type": "meeting-rooms-features",
      "blocks": {
        "d390db88-124b-4e53-b3e8-234749d7ec3a": {
          "type": "features_list_left",
          "settings": {
            "title": "Sound \/ Acoustics",
            "description": "<p><strong>Leading The Sound Pod Industry<\/strong><br\/>STC 30dB (±5dB) &<br\/>RT0.25s (±0.1s)<br\/>Daiki Verified<br\/>ISO 23351-1<\/p><p><\/p>"
          }
        },
        "d3a6f941-b3fc-45f8-b5a6-c82167cfcad9": {
          "type": "features_list_left",
          "settings": {
            "title": "Materials",
            "description": "<p><strong>External Frame<\/strong><br\/>Aviation Type Alloy, Cast Aluminium Corners<br\/>Nanometer PP Finish<strong><br\/>External Panels<br\/><\/strong>Carbon Composite <br\/>≤0.071kg CO2e<br\/><strong>Glass<br\/><\/strong>10mm Tempered Glass Front & Back, Similar To High Speed Trains ≤0.4MJ<br\/><strong>Damping Feet<br\/><\/strong>Vibration control<br\/><strong>Internally<\/strong> <br\/>Acoustic PET board (Recycled Plastic Bottles)<\/p><p><\/p>"
          }
        },
        "82e4437d-c3ed-46e1-8f33-db6645f21ed8": {
          "type": "features_list_left",
          "settings": {
            "title": "Dimensions",
            "description": "<p><strong>Desk Dims.<br\/><\/strong>*Adjustable Desk Width 600mm * Depth 250mm, Lowest Point 750mm, Highest Point 1200mm<\/p><p><strong>External Size:<\/strong><br\/>Height: 230cm Width: 100cm Depth: 93.6cm<strong><br\/>Internal Size:<\/strong><br\/>Height: 214cm Width: 84cm Depth: 90cm<br\/><strong>Weight:<br\/><\/strong>Total: 275kg (Pod)<strong><br\/><\/strong>Total: 299kg (Pod+Furniture)<\/p>"
          }
        },
        "453c3266-3dfc-413c-a2c1-d76c8e57c79d": {
          "type": "features_list_left",
          "settings": {
            "title": "Ventilation",
            "description": "<p>Singular Turbo Fan, Highly efficient airflow at 184m³\/h (max) refreshing air every minute, guaranteeing excellent oxygen rates. A low-noise air system via multi-channel air ducts allows minimum noise disturbance inside and out.<\/p>"
          }
        },
        "cb2a41ec-f379-4bf5-bcce-bde5e4db4e1a": {
          "type": "features_list_right",
          "settings": {
            "title": "Electrical \/ Lighting",
            "description": "<p>*Adjustable Desk 600mm * 250mm, Lowest Point 750mm, Highest Point 1200mm<br\/>*4000k  (150Lx) Daylight LED<br\/>*Multiple Power Outlets, 220- 24V + USB, USB-C<br\/>*Energy Saving Motion Sensor <br\/>*Blue Tooth Audi System<br\/>*Aux Lossless Port<br\/><\/p>"
          }
        },
        "6b66cb21-f9e3-4ec5-89ed-37a0d24af348": {
          "type": "features_list_right",
          "settings": {
            "title": "Optional Furniture",
            "description": "<p>Optional furniture built specifically for these pods is an excellent option for comfort and style. The stool is adjustable via a hydraulic lift; the height adjustable work shelf is plywood style front and white laminated top.<\/p>"
          }
        },
        "19323c70-b1a6-44b1-b5b4-12733c55ae2e": {
          "type": "features_list_right",
          "settings": {
            "title": "Safety",
            "description": "<p><strong>Emergency Safety Hammer<br\/><\/strong>Included For Internal Fitting<br\/><strong>Fire Rated Foam<br\/><\/strong>UK Fire foam where required.<strong> <br\/>Sprinkler \/ Mains Fire Alarm<br\/><\/strong>Laser cut and usable if required<strong> <br\/>Formaldehyde Free<br\/><\/strong>No wood or glue<strong><br\/><\/strong><\/p>"
          }
        },
