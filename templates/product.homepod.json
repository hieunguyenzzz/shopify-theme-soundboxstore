        "d3a6f941-b3fc-45f8-b5a6-c82167cfcad9": {
          "type": "features_list_left",
          "settings": {
            "title": "Materials",
            "description": "<p><strong>Construction<br\/><\/strong>Carbon Composite \/ Honey Comb Panels ≤0.071kg CO2e<br\/><strong>Glass<br\/><\/strong>Similar To High Speed Trains<strong><br\/><\/strong>≤0.4MJ<br\/><strong>Adjustable Feet \/ Coasters<br\/><\/strong>Reduce feet to deploy cosaters to move the Folio Phone Booth<strong><br\/><\/strong><\/p>"
