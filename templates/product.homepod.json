        "82e4437d-c3ed-46e1-8f33-db6645f21ed8": {
          "type": "features_list_left",
          "settings": {
            "title": "Dimensions",
            "description": "<p><strong>Internal Size:<\/strong><br\/>Height: 196cm Width: 100cm Depth: 80cm <strong><br\/>External Size:<\/strong><br\/>Height: 210cm Width: 110cm Depth: 90cm<br\/><strong>Weight:<br\/><\/strong>Total Weight: 224kg (Pod+Furniture)<\/p>"
          }
        },
        "453c3266-3dfc-413c-a2c1-d76c8e57c79d": {
          "type": "features_list_left",
          "settings": {
            "title": "Ventilation",
            "description": "<p>Singular Turbo Fan, Highly efficient airflow at 184m³\/h (max) refreshing air every 40 seconds, guaranteeing excellent oxygen rates. A low-noise air system via multi-channel air ducts allows minimum noise disturbance inside and out.<\/p>"
          }
        },
        "cb2a41ec-f379-4bf5-bcce-bde5e4db4e1a": {
          "type": "features_list_right",
          "settings": {
            "title": "Electrical \/ Lighting",
            "description": "<p>Main Daylight LED *4000k(150Lx)<br\/>Dimmable Directional Task Strip Light <br\/>* UK \/ EURO Power Supply<br\/>- USB * 2, USB-C * 1<br\/>*Energy Saving Motion Sensor Control<\/p>"
