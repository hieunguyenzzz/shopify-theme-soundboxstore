        "453c3266-3dfc-413c-a2c1-d76c8e57c79d": {
          "type": "features_list_left",
          "settings": {
            "title": "Ventilation",
            "description": "<p>Highly efficient air flow at 78 m³\/hr refreshing air every two minutes guaranteeing excellent oxygen rates. With low noise air system via multi channel air ducts allowing minimum noise distrubance inside and out.<\/p>"
          }
        },
        "d390db88-124b-4e53-b3e8-234749d7ec3a": {
          "type": "features_list_left",
          "settings": {
            "title": "Sound \/ Acoustics",
            "description": "<p><strong>Leading The Sound Pod Industry<\/strong><br\/>STC 30dB (±5dB) &<br\/>RT0.25s (±0.1s)<br\/>Daiki Verified<\/p><p><\/p>"
          }
        },
        "cb2a41ec-f379-4bf5-bcce-bde5e4db4e1a": {
          "type": "features_list_right",
          "settings": {
            "title": "Electrical \/ Lighting",
            "description": "<p>*4000k  (150Lx) Daylight LED<br\/>* UK \/ EURO Power Supply<\/p><p>- USB * 2, USB-C * 1<br\/>*Energy Saving Motion Sensor Control<br\/>Directional Strip Task Light<br\/><\/p>"
          }
