      "blocks": {
        "82e4437d-c3ed-46e1-8f33-db6645f21ed8": {
          "type": "features_list_left",
          "settings": {
            "title": "Dimensions",
            "description": "<p><strong>Internal Size:<\/strong><br\/>Height: 213cm Width: 100cm<br\/>Depth: 90cm<strong><br\/>External Size:<\/strong><br\/>Height: 226cm Width: 110cm Depth: 100cm<\/p>"
