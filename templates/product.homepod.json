        "6b66cb21-f9e3-4ec5-89ed-37a0d24af348": {
          "type": "features_list_right",
          "settings": {
            "title": "Furniture",
            "description": "<p>The Folio comes with mini sofa, task desk, and task light designed ergonomically for working on laptop, phone booth, zoom \/ conference calls. <\/p>"
          }
        },
        "19323c70-b1a6-44b1-b5b4-12733c55ae2e": {
