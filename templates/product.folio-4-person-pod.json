    "meeting-rooms-features": {
      "type": "meeting-rooms-features",
      "blocks": {
        "82e4437d-c3ed-46e1-8f33-db6645f21ed8": {
          "type": "features_list_left",
          "settings": {
            "title": "Dimensions",
            "description": "<p><strong>Internal Size:<\/strong><br\/>Height: 196cm Width: 210cm Depth: 112cm <strong><br\/>External Size:<\/strong><br\/>Height: 225cm Width: 220cm Depth: 122cm<br\/><strong>Weight:<br\/><\/strong>Total Weight: 448kg (Pod+Furniture)<\/p>"
          }
        },
        "d390db88-124b-4e53-b3e8-234749d7ec3a": {
          "type": "features_list_left",
          "settings": {
            "title": "Sound \/ Acoustics",
            "description": "<p><strong>Leading The Sound Pod Industry<\/strong><br\/>STC 30dB (±5dB) &<br\/>RT0.25s (±0.1s)<br\/>Daiki Verified<\/p><p><\/p>"
          }
        },
        "d3a6f941-b3fc-45f8-b5a6-c82167cfcad9": {
          "type": "features_list_left",
          "settings": {
            "title": "Materials",
            "description": "<p><strong>Construction<br\/><\/strong>Carbon Composite \/ Honey Comb Panels ≤0.071kg CO2e<br\/><strong>Glass<br\/><\/strong>Similar To High Speed Trains<strong><br\/><\/strong>≤0.4MJ<br\/><strong>Adjustable Feet \/ Coasters<br\/><\/strong>Reduce feet to deploy wheels to move the Folio Phone Booth<strong><br\/><\/strong><\/p>"
          }
        },
        "453c3266-3dfc-413c-a2c1-d76c8e57c79d": {
          "type": "features_list_left",
          "settings": {
            "title": "Ventilation",
            "description": "<p>Dual Turbo Fan, Highly efficient airflow at 184m³\/h (max) refreshing air every 40 seconds, guaranteeing excellent oxygen rates. A low-noise air system via multi-channel air ducts allows minimum noise disturbance inside and out.<\/p>"
          }
        },
        "cb2a41ec-f379-4bf5-bcce-bde5e4db4e1a": {
          "type": "features_list_right",
          "settings": {
            "title": "Electrical \/ Lighting",
            "description": "<p>Main Daylight LED *4000k(150Lx)<br\/>2 * 220-240v UK \/ EURO Power Supply<br\/>2 * Ethernet (Cat 6) Connectors<br\/>2* USB, 2 * USB-C<br\/>Energy Saving Motion Sensor Control<\/p>"
          }
        },
        "6b66cb21-f9e3-4ec5-89ed-37a0d24af348": {
          "type": "features_list_right",
          "settings": {
            "title": "Furniture",
            "description": "<p>The Folio 4 Person Meeting Booth comes with mini sofa, task desk, and non-directional main daylight LED designed ergonomically for working on laptop, meeting booth, zoom \/ conference calls. <\/p>"
          }
        },
        "19323c70-b1a6-44b1-b5b4-12733c55ae2e": {
          "type": "features_list_right",
          "settings": {
            "title": "Safety",
            "description": "<p><strong>Emergency Safety Hammer<br\/><\/strong>Included For Internal Fitting<br\/><br\/><strong>Fire Rated Foam<br\/><\/strong>UK Fire foam where required.<strong> <\/strong><\/p>"
          }
        },
        "aade32d9-6806-430b-b9c3-07623b95803d": {
