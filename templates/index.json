        "274907dc-3e29-4235-af23-66c46219173a": {
          "type": "image_section",
          "settings": {
            "image": "shopify:\/\/shop_images\/our-mission.jpg",
            "title": "Our Mission",
            "description": "<p>As one of the largest acoustic solution companies in Europe & Asia, our mission is to fulfill the needs of a comfortable, sustainable, and safe sound pod\/booth environment.<\/p>"
          }
        },
