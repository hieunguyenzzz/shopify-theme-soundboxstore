      "blocks": {
        "9d8aec96-c9d8-4b5b-b03e-f8152828e3ba": {
          "type": "image_section",
          "settings": {
            "image": "shopify:\/\/shop_images\/sound-box-insitu-1.png",
            "title": "Our Philosophy",
            "description": "<p>In partnering with professionals from different industry sectors to deliver consistent, reliable, and competitive acoustic control, insulation, and damping solutions. We are in the business to inspire and open up a new era of infinite possibilities.<\/p>"
